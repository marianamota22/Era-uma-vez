        <agent text="onde canta o sabiá;" color="blue" x="15" y="380"/>
        <agent text="as aves, que aqui gorjeiam," color="blue" x="15" y="360"/>
        <agent text="não gorjeiam como lá." color="blue" x="15" y="340"/>
